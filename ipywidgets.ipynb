    "from IPython.display import display\n",
    "\n",
    "slider = widgets.IntSlider(\n",
    "    value=50,\n",
    "    min=0,\n",
    "    max=100,\n",
    "    step=1,\n",
    "    description='Slider:'\n",
    ")\n",
    "display(slider)"
